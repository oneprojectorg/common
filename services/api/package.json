    "server-only": "^0.0.1",
    "spacetime": "^7.7.0",
    "ssr-only-secrets": "^0.1.4",
    "superjson": "^2.2.2",
    "trpc-to-openapi": "^3.1.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@op/typescript-config": "workspace:*",
    "@types/jsonwebtoken": "^9.0.10",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "jsonwebtoken": "^9.0.2",
