{
  "name": "@op/api",
  "version": "0.0.0",
  "private": true,
  "license": "SEE LICENSE in /LICENSE",
  "type": "module",
  "exports": {
    ".": "./index.ts",
    "./client": "./src/TRPCProvider.tsx",
    "./serverClient": "./src/serverClient.ts",
    "./ssrCookies": "./src/ssrCookies.ts",
    "./encoders": "./src/encoders/index.ts"
  },
  "scripts": {
    "test": "vitest",
    "test:check-supabase": "tsx src/test/check-supabase.ts",
    "test:coverage": "vitest --coverage",
    "test:db:reset": "pnpm test:supabase:reset && pnpm test:migrate",
    "test:integration": "tsx src/test/check-supabase.ts && vitest run src/test/integration/",
    "test:integration:migrate": "tsx src/test/check-supabase.ts --migrations && vitest run src/test/integration/",
    "test:integration:watch": "tsx src/test/check-supabase.ts && vitest watch src/test/integration/",
    "test:migrate": "tsx src/test/check-supabase.ts --migrations",
    "test:supabase:reset": "tsx src/test/supabase-test.ts reset",
    "test:supabase:start": "tsx src/test/supabase-test.ts start",
    "test:supabase:status": "tsx src/test/supabase-test.ts status",
    "test:supabase:stop": "tsx src/test/supabase-test.ts stop",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "typecheck": "pnpm tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.1",
    "@op/analytics": "workspace:*",
