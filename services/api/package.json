    "@op/db": "workspace:*",
    "@op/events": "workspace:*",
    "@op/logging": "workspace:*",
    "@op/realtime": "workspace:^",
    "@op/supabase": "workspace:*",
    "@op/types": "workspace:*",
    "@supabase/supabase-js": "^2.49.3",
    "@tanstack/query-sync-storage-persister": "5.66.11",
    "@tanstack/react-query": "5.66.11",
    "@tanstack/react-query-persist-client": "5.66.11",
    "@trpc/client": "11.6.0",
    "@trpc/react-query": "11.6.0",
    "@trpc/server": "11.6.0",
    "@types/sanitize-s3-objectkey": "^0.0.0",
    "@vercel/functions": "^2.1.0",
    "access-zones": "0.0.1",
    "ai": "^4.2.2",
    "cookie": "^1.0.2",
    "drizzle-orm": "^0.40.0",
    "drizzle-zod": "^0.8.3",
    "fast-xml-parser": "^5.2.1",
    "nanoid": "5.1.2",
